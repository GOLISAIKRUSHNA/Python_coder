    "#right rotate\n",
    "\n",
    "n=int(input())\n",
    "k=int(input(\"rotate right\"))\n",
    "\n",
    "li=list(map(int,input().split()))\n",
    "\n",
    "k=k%n\n",
    "li[:]=li[n-k:]+li[:n-k]\n",
    "print(li[:])\n",
    "print(\"after k rotate at right\")"
   ]
  }
